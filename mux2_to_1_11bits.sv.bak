
module mux2_to_1_11BIT(out,inputA,inputB,sel); // signal multiplexing between 2 11-Bit words.

output logic [11:0] out;
input [11:0] inputA;
input [11:0] inputB;
input sel;

always @(inputA or inputB or sel) begin
  
  if (sel == 0) begin
	out <= inputA;
  end
  else begin
	out <= inputB; 
  end
end
endmodule